{
  "title": "Simulate right-fn on QMK keyboards with right-cmd",
  "rules": [
    {
      "description": "Change RCMD+asdf to |_=\\",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "backslash"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "backslash",
              "modifiers": [
                "shift"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "hyphen",
              "modifiers": [
                "shift"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ]
        }
      ]
    }
  ]
}
