{
  "title": "Change cmd-fn to cmd-`",
  "rules": [
    {
      "description": "Change cmd+fn to cmd-` (app switch)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "fn",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    }
  ]
}
